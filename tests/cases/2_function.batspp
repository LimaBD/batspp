$ function fibonacci () {
$
$    # this prints the first N Fibonacci numbers
$
$    result=""
$    a=0
$    b=1
$    for (( i=0; i<=$1; i++ ))
$    do
$        result="$result$a "
$        fn=$((a + b))
$        a=$b
$        b=$fn
$    done
$    echo $result
$ }
